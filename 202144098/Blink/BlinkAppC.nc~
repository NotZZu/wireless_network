configuration BlinkAppC
{
}
implementation
{
        components MainC, BlinkC, LedsC;
        components TimerMilliC() as Timer0;
        components TimerMilliC() as Timer1;
        components TimerMilliC() as Timer2;

        
        BlinkC -> MainC.Boot;

        BlinkC.Timer0 -> Timer0;
        BlinkC.Timer1 -> Timer1;
        BlinkC.Timer2 -> Timer2;
        BlinkC.Leds -> LedsC;
}
